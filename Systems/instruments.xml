<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<filter>
		<name>Indicated airspeed (miles per hour)</name>
		<type>gain</type>
		<gain>1.1507794</gain>
		<input>/instrumentation/airspeed-indicator/indicated-speed-kt</input>
		<output>/instrumentation/airspeed-indicator/indicated-speed-mph</output>
	</filter>
	
	<filter>
		<name>Suction gauge</name>
		<type>gain</type>
		<enable>
			<property>/instrumentation/suction/serviceable</property>
		</enable>
		<input>/systems/vacuum/gauge-suction-inhg</input>
		<output>/instrumentation/suction/indicated-suction-inhg</output>
	</filter>
</PropertyList>
