<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>c310a.ac</path>
	
	<defaults>
		<axis-animation-vertex-order-xyz/>
	</defaults>
	
	<model>
		<usage>interior</usage>
		<path>Interior/interior.xml</path>
	</model>
	
	<!-- lights -->
	<model>
		<path>Lights/nav-light-left.xml</path>
		<offsets>
			<x-m>-0.044</x-m>
			<y-m>-5.53</y-m>
			<z-m>-0.37</z-m>
		</offsets>
	</model>
	<model>
		<path>Lights/nav-light-right.xml</path>
		<offsets>
			<x-m>-0.044</x-m>
			<y-m>5.53</y-m>
			<z-m>-0.37</z-m>
		</offsets>
	</model>
	<model>
		<path>Lights/nav-light-tail.xml</path>
		<offsets>
			<x-m>6.618</x-m>
			<y-m>0</y-m>
			<z-m>-0.19</z-m>
		</offsets>
	</model>
	<model>
		<name>BeaconLight</name>
		<path>Lights/beacon-light.xml</path>
		<offsets>
			<x-m>5.57</x-m>
			<y-m>0</y-m>
			<z-m>1.975</z-m>
		</offsets>
	</model>
	<model>
		<name>LeftLandingLight</name>
		<path>Lights/landing-light.xml</path>
		<offsets>
			<x-m>1.006</x-m>
			<y-m>-3.493</y-m>
			<z-m>-0.608</z-m>
			<roll-deg>-4.5</roll-deg>
		</offsets>
		<overlay>
			<params>
				<brightness-prop>sim/model/lights/landing[0]/brightness-norm</brightness-prop>
			</params>
		</overlay>
	</model>
	<model>
		<name>RightLandingLight</name>
		<path>Lights/landing-light.xml</path>
		<offsets>
			<x-m>1.006</x-m>
			<y-m>3.493</y-m>
			<z-m>-0.608</z-m>
			<roll-deg>4.5</roll-deg>
		</offsets>
		<overlay>
			<params>
				<brightness-prop>sim/model/lights/landing[1]/brightness-norm</brightness-prop>
			</params>
		</overlay>
	</model>
	
	<!-- control surfaces -->
	<animation>
		<type>rotate</type>
		<object-name>LeftAileron</object-name>
		<object-name>AileronTrimTab</object-name>
		<object-name>AileronTrimTabAxis</object-name>
		<axis>
			<object-name>LeftAileronAxis</object-name>
		</axis>
		<property>surface-positions/left-aileron-deg</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>AileronTrimTab</object-name>
		<axis>
			<object-name>AileronTrimTabAxis</object-name>
		</axis>
		<property>surface-positions/aileron-trim-tab-deg</property>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RightAileron</object-name>
		<axis>
			<object-name>RightAileronAxis</object-name>
		</axis>
		<property>surface-positions/right-aileron-deg</property>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>ElevatorTrimTab</object-name>
		<object-name>ElevatorTrimTabAxis</object-name>
		<object-name>LeftElevator</object-name>
		<object-name>RightElevator</object-name>
		<axis>
			<object-name>ElevatorAxis</object-name>
		</axis>
		<property>surface-positions/elevator-deg</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>ElevatorTrimTab</object-name>
		<axis>
			<object-name>ElevatorTrimTabAxis</object-name>
		</axis>
		<property>surface-positions/elevator-trim-tab-deg</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Rudder</object-name>
		<object-name>RudderTrimTab</object-name>
		<object-name>RudderTrimTabAxis</object-name>
		<axis>
			<object-name>RudderAxis</object-name>
		</axis>
		<property>surface-positions/rudder-deg</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RudderTrimTab</object-name>
		<axis>
			<object-name>RudderTrimTabAxis</object-name>
		</axis>
		<property>surface-positions/rudder-trim-tab-deg</property>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>LeftOutboardFlap</object-name>
		<axis>
			<object-name>LeftOutboardFlapAxis</object-name>
		</axis>
		<property>surface-positions/flaps-deg</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>LeftInboardFlap</object-name>
		<axis>
			<object-name>LeftInboardFlapAxis</object-name>
		</axis>
		<property>surface-positions/flaps-deg</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RightInboardFlap</object-name>
		<axis>
			<object-name>RightInboardFlapAxis</object-name>
		</axis>
		<property>surface-positions/flaps-deg</property>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RightOutboardFlap</object-name>
		<axis>
			<object-name>RightOutboardFlapAxis</object-name>
		</axis>
		<property>surface-positions/flaps-deg</property>
	</animation>
	
	<!-- propellers -->
	<model>
		<name>LeftPropellerBlur</name>
		<path>Effects/prop-blur.xml</path>
		<offsets>
			<x-m>-0.566</x-m>
			<y-m>-1.947</y-m>
			<z-m>-0.474</z-m>
		</offsets>
		<overlay>
			<params>
				<diameter-m>2.13</diameter-m>
				<num-blades>2</num-blades>
				<rpm-property>engines/engine[0]/thruster/rpm</rpm-property>
				<pitch-property>engines/engine[0]/thruster/pitch</pitch-property>
				<blade-width n="0">0.07</blade-width>
				<blade-width n="1">0.12</blade-width>
				<blade-width n="2">0.16</blade-width>
				<blade-width n="3">0.14</blade-width>
			</params>
		</overlay>
	</model>
	<model>
		<name>RightPropellerBlur</name>
		<path>Effects/prop-blur.xml</path>
		<offsets>
			<x-m>-0.566</x-m>
			<y-m>1.947</y-m>
			<z-m>-0.474</z-m>
		</offsets>
		<overlay>
			<params>
				<diameter-m>2.13</diameter-m>
				<num-blades>2</num-blades>
				<rpm-property>engines/engine[1]/thruster/rpm</rpm-property>
				<pitch-property>engines/engine[1]/thruster/pitch</pitch-property>
				<blade-width n="0">0.07</blade-width>
				<blade-width n="1">0.12</blade-width>
				<blade-width n="2">0.16</blade-width>
				<blade-width n="3">0.14</blade-width>
			</params>
		</overlay>
	</model>
	
	<animation>
		<condition>
			<less-than>
				<property>engines/engine[0]/thruster/rpm</property>
				<value>125</value>
			</less-than>
		</condition>
		<type>select</type>
		<object-name>LeftPropellerBlade1</object-name>
		<object-name>LeftPropellerBlade2</object-name>
	</animation>
	
	<animation>
		<condition>
			<less-than>
				<property>engines/engine[1]/thruster/rpm</property>
				<value>125</value>
			</less-than>
		</condition>
		<type>select</type>
		<object-name>RightPropellerBlade1</object-name>
		<object-name>RightPropellerBlade2</object-name>
	</animation>
	
	<animation>
		<type>spin</type>
		<object-name>LeftPropellerBlur</object-name>
		<object-name>LeftPropellerSpinner</object-name>
		<object-name>LeftPropellerBlade1</object-name>
		<object-name>LeftPropellerBlade1Axis</object-name>
		<object-name>LeftPropellerBlade2</object-name>
		<object-name>LeftPropellerBlade2Axis</object-name>
		<axis>
			<object-name>LeftPropellerSpinAxis</object-name>
		</axis>
		<property>engines/engine/thruster[0]/rpm</property>
	</animation>
	
	<animation>
		<type>spin</type>
		<object-name>RightPropellerBlur</object-name>
		<object-name>RightPropellerSpinner</object-name>
		<object-name>RightPropellerBlade1</object-name>
		<object-name>RightPropellerBlade1Axis</object-name>
		<object-name>RightPropellerBlade2</object-name>
		<object-name>RightPropellerBlade2Axis</object-name>
		<axis>
			<object-name>RightPropellerSpinAxis</object-name>
		</axis>
		<property>engines/engine[1]/thruster/rpm</property>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>LeftPropellerBlade1</object-name>
		<axis>
			<object-name>LeftPropellerBlade1Axis</object-name>
		</axis>
		<property>engines/engine[0]/thruster/pitch</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>LeftPropellerBlade2</object-name>
		<axis>
			<object-name>LeftPropellerBlade2Axis</object-name>
		</axis>
		<property>engines/engine[0]/thruster/pitch</property>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RightPropellerBlade1</object-name>
		<axis>
			<object-name>RightPropellerBlade1Axis</object-name>
		</axis>
		<property>engines/engine[1]/thruster/pitch</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RightPropellerBlade2</object-name>
		<axis>
			<object-name>RightPropellerBlade2Axis</object-name>
		</axis>
		<property>engines/engine[1]/thruster/pitch</property>
	</animation>
	
	<!-- nose gear -->
	<animation>
		<type>rotate</type>
		<object-name>NoseWheelHinge</object-name>
		<object-name>NoseWheelRim</object-name>
		<object-name>NoseWheelTyre</object-name>
		<object-name>NoseGearUpperTorqueLink</object-name>
		<object-name>NoseGearStrut</object-name>
		<object-name>NoseGearMudguard</object-name>
		<axis>
			<object-name>NoseGearSteeringAxis</object-name>
		</axis>
		<property>gear/gear/steering-deg</property>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>NoseWheelHinge</object-name>
		<object-name>NoseWheelRim</object-name>
		<object-name>NoseWheelTyre</object-name>
		<object-name>NoseGearUpperTorqueLink</object-name>
		<object-name>NoseGearStrut</object-name>
		<object-name>NoseGearStrutBarrel</object-name>
		<object-name>NoseGearCylinder</object-name>
		<object-name>NoseGearMudguard</object-name>
		<axis>
			<object-name>NoseGearStrutAxis</object-name>
		</axis>
		<property>gear/gear/position-norm</property>
		<factor>-102</factor>
		<offset-deg>102</offset-deg>
	</animation>
	
	<animation>
		<type>locked-track</type>
		<object-name>NoseGearActuatorDragBrace</object-name>
		<center>
			<object-name>NoseGearActuatorDragBraceAxis</object-name>
		</center>
		<lock-axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</lock-axis>
		<track-axis>
			<x>0</x>
			<y>1</y>
			<z>0.8</z>
		</track-axis>
		
		<target-name>NoseGearStrutBarrel</target-name>
		<target-center>
			<object-name>NoseGearActuatorDragRodAxis</object-name>
		</target-center>
		
		<slave-name>NoseGearActuatorDragRod</slave-name>
		<slave-center>
			<object-name>NoseGearActuatorDragRodBraceAxis</object-name>
		</slave-center>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>NoseWheelHinge</object-name>
		<object-name>NoseWheelRim</object-name>
		<object-name>NoseWheelTyre</object-name>
		<object-name>NoseGearCylinder</object-name>
		<object-name>NoseGearMudguard</object-name>
		<axis>
			<object-name>NoseGearSteeringAxis</object-name>
		</axis>
		<property>gear/gear/compression-m</property>
		<factor>-1</factor>
	</animation>
	
	<animation>
		<type>spin</type>
		<object-name>NoseWheelRim</object-name>
		<object-name>NoseWheelTyre</object-name>
		<axis>
			<object-name>NoseWheelAxis</object-name>
		</axis>
		<property>gear/gear/rollspeed-ms</property>
		<factor>24.175</factor>
	</animation>
	
	<animation>
		<type>locked-track</type>
		<object-name>NoseGearUpperTorqueLink</object-name>
		<center> <!-- upper hinge -->
			<object-name>NoseGearUpperTorqueLinkAxis</object-name>
		</center>
		<lock-axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</lock-axis>
		<track-axis> <!-- vector between upper and lower end of the oleo strut - does not need to be normalized -->
			<object-name>NoseGearSteeringAxis</object-name>
		</track-axis>
		
		<target-name>NoseGearCylinder</target-name>
		<target-center>	<!-- lower hinge -->
			<object-name>NoseGearLowerTorqueLinkAxis</object-name>
		</target-center>
		
		<slave-name>NoseGearLowerTorqueLink</slave-name>
		<slave-center>	<!-- middle hinge -->
			<object-name>NoseGearMiddleTorqueLinkAxis</object-name>
		</slave-center>
	</animation>
	
	<!-- left main gear -->
	<animation>
		<type>rotate</type>
		<object-name>LeftMainWheelHinge</object-name>
		<object-name>LeftMainWheelRim</object-name>
		<object-name>LeftMainWheelTyre</object-name>
		<object-name>LeftBrake</object-name>
		<object-name>LeftMainGearUpperTorqueLink</object-name>
		<object-name>LeftMainGearStrut</object-name>
		<object-name>LeftMainGearCylinder</object-name>
		<object-name>LeftMainGearLowerSideBrace</object-name>
		<axis>
			<object-name>LeftMainGearStrutAxis</object-name>
		</axis>
		<property>gear/gear[1]/position-norm</property>
		<factor>85</factor>
		<offset-deg>-85</offset-deg>
	</animation>
	
	<animation>
		<type>locked-track</type>
		<object-name>LeftMainGearLowerSideBrace</object-name>
		<center>
			<object-name>LeftMainGearLowerSideBraceAxis</object-name>
		</center>
		<lock-axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</lock-axis>
		<track-axis>
			<x>0</x>
			<y>1</y>
			<z>0.8</z>
		</track-axis>
		
		<target-name>LeftMainGearSideBraceMount</target-name>
		<target-center>
			<object-name>LeftMainGearUpperSideBraceAxis</object-name>
		</target-center>
		
		<slave-name>LeftMainGearUpperSideBrace</slave-name>
		<slave-center>
			<object-name>LeftMainGearMiddleSideBraceAxis</object-name>
		</slave-center>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>LeftMainWheelHinge</object-name>
		<object-name>LeftMainWheelRim</object-name>
		<object-name>LeftMainWheelTyre</object-name>
		<object-name>LeftBrake</object-name>
		<object-name>LeftMainGearCylinder</object-name>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
		<property>gear/gear[1]/compression-m</property>
	</animation>
	
	<animation>
		<type>spin</type>
		<object-name>LeftMainWheelRim</object-name>
		<object-name>LeftMainWheelTyre</object-name>
		<axis>
			<object-name>LeftMainWheelAxis</object-name>
		</axis>
		<property>gear/gear[1]/rollspeed-ms</property>
		<factor>19.1</factor>
	</animation>
	
	<animation>
		<type>locked-track</type>
		<object-name>LeftMainGearUpperTorqueLink</object-name>
		<center> <!-- upper hinge -->
			<object-name>LeftMainGearUpperTorqueLinkAxis</object-name>
		</center>
		<lock-axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</lock-axis>
		<track-axis> <!-- vector between upper and lower end of the oleo strut - does not need to be normalized -->
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</track-axis>
		
		<target-name>LeftMainGearCylinder</target-name>
		<target-center> <!-- lower hinge -->
			<object-name>LeftMainGearLowerTorqueLinkAxis</object-name>
		</target-center>
		
		<slave-name>LeftMainGearLowerTorqueLink</slave-name>
		<slave-center> <!-- middle hinge -->
			<object-name>LeftMainGearMiddleTorqueLinkAxis</object-name>
		</slave-center>
	</animation>
	
	<!-- right main gear -->
	<animation>
		<type>rotate</type>
		<object-name>RightMainWheelHinge</object-name>
		<object-name>RightMainWheelRim</object-name>
		<object-name>RightMainWheelTyre</object-name>
		<object-name>RightBrake</object-name>
		<object-name>RightMainGearUpperTorqueLink</object-name>
		<object-name>RightMainGearStrut</object-name>
		<object-name>RightMainGearCylinder</object-name>
		<object-name>RightMainGearLowerSideBrace</object-name>
		<axis>
			<object-name>RightMainGearStrutAxis</object-name>
		</axis>
		<property>gear/gear[2]/position-norm</property>
		<factor>-85</factor>
		<offset-deg>85</offset-deg>
	</animation>
	
	<animation>
		<type>locked-track</type>
		<object-name>RightMainGearLowerSideBrace</object-name>
		<center>
			<object-name>RightMainGearLowerSideBraceAxis</object-name>
		</center>
		<lock-axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</lock-axis>
		<track-axis>
			<x>0</x>
			<y>1</y>
			<z>-0.8</z>
		</track-axis>
		
		<target-name>RightMainGearSideBraceMount</target-name>
		<target-center>
			<object-name>RightMainGearUpperSideBraceAxis</object-name>
		</target-center>
		
		<slave-name>RightMainGearUpperSideBrace</slave-name>
		<slave-center>
			<object-name>RightMainGearMiddleSideBraceAxis</object-name>
		</slave-center>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>RightMainWheelHinge</object-name>
		<object-name>RightMainWheelRim</object-name>
		<object-name>RightMainWheelTyre</object-name>
		<object-name>RightBrake</object-name>
		<object-name>RightMainGearCylinder</object-name>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
		<property>gear/gear[2]/compression-m</property>
	</animation>
	
	<animation>
		<type>spin</type>
		<object-name>RightMainWheelRim</object-name>
		<object-name>RightMainWheelTyre</object-name>
		<axis>
			<object-name>RightMainWheelAxis</object-name>
		</axis>
		<property>gear/gear[2]/rollspeed-ms</property>
		<factor>19.1</factor>
	</animation>
	
	<animation>
		<type>locked-track</type>
		<object-name>RightMainGearUpperTorqueLink</object-name>
		<center> <!-- upper hinge -->
			<object-name>RightMainGearUpperTorqueLinkAxis</object-name>
		</center>
		<lock-axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</lock-axis>
		<track-axis> <!-- vector between upper and lower end of the oleo strut - does not need to be normalized -->
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</track-axis>
		
		<target-name>RightMainGearCylinder</target-name>
		<target-center> <!-- lower hinge -->
			<object-name>RightMainGearLowerTorqueLinkAxis</object-name>
		</target-center>
		
		<slave-name>RightMainGearLowerTorqueLink</slave-name>
		<slave-center> <!-- middle hinge -->
			<object-name>RightMainGearMiddleTorqueLinkAxis</object-name>
		</slave-center>
	</animation>
	
	<!-- gear doors -->
	<animation>
		<type>rotate</type>
		<object-name>NoseGearDoorForward</object-name>
		<axis>
			<object-name>NoseGearDoorForwardAxis</object-name>
		</axis>
		<property>gear/gear/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.2</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>-80</dep>
			</entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>NoseGearDoorRearLeft</object-name>
		<axis>
			<object-name>NoseGearDoorRearLeftAxis</object-name>
		</axis>
		<property>gear/gear/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
