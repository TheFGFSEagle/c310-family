	<animation>
		<condition>
			<less-than>
				<property>engines/engine[0]/thruster/rpm</property>
				<value>125</value>
			</less-than>
		</condition>
		<type>select</type>
		<object-name>LeftPropellerBlade1</object-name>
		<object-name>LeftPropellerBlade2</object-name>
