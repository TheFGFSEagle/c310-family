<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>asi.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>Needle</object-name>
		<emission>
			<red>0.95</red>
			<green>1</green>
			<blue>1</blue>
			<factor>
				<expression>
					<sum>
						<product>
							<property>sim/model/lights/uv-instrument[0]/brightness-norm</property>
							<value>0.65</value>
						</product>
						<product>
							<property>sim/model/lights/uv-instrument[1]/brightness-norm</property>
							<value>0.35</value>
						</product>
					</sum>
				</expression>
			</factor>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Needle</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/airspeed-indicator/indicated-speed-mph</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>40</ind>
				<dep>20</dep>
			</entry>
			<entry>
				<ind>200</ind>
				<dep>276</dep>
			</entry>
			<entry>
				<ind>300</ind>
				<dep>340</dep>
			</entry>
		</interpolation>
	</animation>
	
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>Glass</object-name>
	</effect>
</PropertyList>
