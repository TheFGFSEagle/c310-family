<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>stall-warning.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<emission>
			<red>0.95</red>
			<green>1</green>
			<blue>1</blue>
			<factor>
				<expression>
					<sum>
						<product>
							<property>sim/model/lights/uv-instrument[0]/brightness-norm</property>
							<value>0.3</value>
						</product>
						<product>
							<property>sim/model/lights/uv-instrument[1]/brightness-norm</property>
							<value>0.7</value>
						</product>
					</sum>
				</expression>
			</factor>
		</emission>
	</animation>
	<animation>
		<type>material</type>
		<object-name>LampLens</object-name>
		<emission>
			<red>1</red>
			<green>0</green>
			<blue>0</blue>
			<factor-prop>instrumentation/stall-warning/lamp-brightness-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>material</type>
		<object-name>LampBulb</object-name>
		<emission>
			<red>0.8</red>
			<green>0</green>
			<blue>0</blue>
			<factor-prop>instrumentation/stall-warning/lamp-brightness-norm</factor-prop>
		</emission>
	</animation>
	
	<light>
		<name>Lamp</name>
		<type>spot</type>
		<position>
			<x-m>0.0145</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</position>
		<direction>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</direction>
		<ambient>
			<r>0.09</r>
			<g>0.03</g>
			<b>0.03</b>
			<a>1</a>
		</ambient>
		<diffuse>
			<r>1.0</r>
			<g>0.0</g>
			<b>0.0</b>
			<a>1</a>
		</diffuse>
		<specular>
			<r>1.0</r>
			<g>0.0</g>
			<b>0.0</b>
			<a>1</a>
			</specular>
		<attenuation>
			<c>0</c>
			<l>10</l>
			<q>80</q>
		</attenuation>
		<range-m>0.2</range-m>
		<spot-exponent>1</spot-exponent>
		<spot-cutoff>89.9</spot-cutoff>
		<dim-factor>
			<property>instrumentation/stall-warning/lamp-brightness-norm</property>
		</dim-factor>
	</light>
	
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>LampBulb</object-name>
		<object-name>LampLens</object-name>
	</effect>
</PropertyList>
