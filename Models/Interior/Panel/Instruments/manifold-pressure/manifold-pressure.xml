<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>manifold-pressure.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>Needle1</object-name>
		<object-name>Needle2</object-name>
		<emission>
			<red>0.95</red>
			<green>1</green>
			<blue>1</blue>
			<factor>
				<expression>
					<sum>
						<product>
							<property>sim/model/lights/uv-instrument[0]/brightness-norm</property>
							<value>0.48</value>
						</product>
						<product>
							<property>sim/model/lights/uv-instrument[1]/brightness-norm</property>
							<value>0.52</value>
						</product>
					</sum>
				</expression>
			</factor>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Needle1</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/manifold-pressure/indicated-manifold-pressure-inhg[0]</property>
		<factor>5.4</factor>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Needle2</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/manifold-pressure/indicated-manifold-pressure-inhg[1]</property>
		<factor>5.4</factor>
	</animation>
	
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>Glass</object-name>
	</effect>
</PropertyList>
