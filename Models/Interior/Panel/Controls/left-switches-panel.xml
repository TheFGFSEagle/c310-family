				<property>controls/cabin-air/ventilation</property>
				<tooltip-id>cabin-ventilation-switch</tooltip-id>
				<label>Cabin ventilation: %s</label>
				<script>return ["off", "heat", "fan"][arg[0]];</script>
				<measure-text>Cabin ventilation: heat</measure-text>
			</params>
		</overlay>
	</model>
	
