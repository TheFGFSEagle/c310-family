	<model>
		<name>LeftLandingLightSwitch</name>
		<path>switches/three-position.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0.051</y-m>
			<z-m>0.0725</z-m>
		</offsets>
		<overlay>
			<params>
				<property>controls/lighting/landing-light[0]</property>
				<tooltip-id>left-landing-light-switch</tooltip-id>
				<label>Left landing light: %s</label>
				<script>return ["off", "extend / on", "retract"][arg[0]];</script>
				<measure-text>Left landing light: extend and light</measure-text>
			</params>
		</overlay>
	</model>
	
	<model>
		<name>RightLandingLightSwitch</name>
		<path>switches/three-position.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0.072</y-m>
			<z-m>0.0725</z-m>
		</offsets>
