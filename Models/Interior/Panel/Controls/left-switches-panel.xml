	<model>
		<name>LeftStarterButton</name>
		<path>starter-button/starter-button.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0.12</y-m>
			<z-m>0.0725</z-m>
		</offsets>
		<overlay>
			<params>
				<property>controls/engines/engine[0]/starter-button</property>
				<tooltip-id>left-starter-button</tooltip-id>
				<label>Left starter: %s</label>
				<measure-text>Left starter: off</measure-text>
			</params>
		</overlay>
	</model>
	
	<model>
		<name>PrimeSwitch</name>
		<path>switches/three-position.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0.145</y-m>
			<z-m>0.08</z-m>
			<roll-deg>-90</roll-deg>
		</offsets>
		<overlay>
			<params>
				<property>controls/engines/primer</property>
				<tooltip-id>primer-switch</tooltip-id>
				<label>Primer: %s</label>
				<script>return ["off", "right engine", "left engine"][arg[0]];</script>
				<measure-text>Primer: right engine</measure-text>
				<spring-loaded type="bool">true</spring-loaded>
			</params>
		</overlay>
	</model>
	
	<model>
		<name>RightStarterButton</name>
		<path>starter-button/starter-button.xml</path>
		<offsets>
