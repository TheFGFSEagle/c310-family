<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<model>
		<name>AlternatorsBatterySwitchBar</name>
		<path>switches/three-switch-bar.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0.034</y-m>
