<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>blade-blur.ac</path>
	<params>
		<rpm-prop>engines/engine[0]/thruster/rpm</rpm-prop>
		<pitch-prop>engines/engine[0]/thruster/pitch</pitch-prop>
		<blur-onset-rpm>100</blur-onset-rpm>
		<blur-full-rpm>1000</blur-full-rpm>
		<mesh-number>1</mesh-number>
		<total-mesh-number>100</total-mesh-number>
		<sense>1</sense>
		<num-blades>2</num-blades>
		<transparency>0.05</transparency>
	</params>
	
	<effect>
		<inherits-from>Effects/exterior-glass</inherits-from>
		<object-name>PropellerBlade1</object-name>
		<object-name>PropellerBlade2</object-name>
	</effect>
	
	<animation>
		<type>material</type>
		<object-name>PropellerBlade1</object-name>
		<object-name>PropellerBlade2</object-name>
		<transparency>
			<alpha alias="/params/transparency"/>
		</transparency>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>PropellerBlade1</object-name>
		<object-name>PropellerBlade2</object-name>
		<axis>
			<x alias="/params/sense"/>
			<y>0</y>
			<z>0</z>
		</axis>
		<expression>
			<product>
				<div>
					<value>360</value>
					<value alias="/params/num-blades"/>
				</div>
				<table>
					<property alias="/params/rpm-prop"/>
					<entry>
						<ind alias="/params/blur-onset-rpm"/>
						<dep>0</dep>
					</entry>
					<entry>
						<ind alias="/params/blur-full-rpm"/>
						<dep>1</dep>
					</entry>
				</table>
				<div>
					<value alias="/params/mesh-number"/>
					<value alias="/params/total-mesh-number"/>
				</div>
			</product>
		</expression>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>PropellerBlade1</object-name>
		<axis>
			<x>0</x>
			<y alias="/params/sense"/>
			<z>0</z>
		</axis>
		<property alias="/params/pitch-prop"/>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>PropellerBlade2</object-name>
		<axis>
			<x>0</x>
			<y alias="/params/sense"/>
			<z>0</z>
		</axis>
		<property alias="/params/pitch-prop"/>
		<factor>-1</factor>
	</animation>
</PropertyList>
